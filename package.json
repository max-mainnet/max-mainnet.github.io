{
  "name": "parcel-typescript-react-tailwind",
  "version": "3.2.0",
  "license": "MIT",
  "description": "Trying-out Tailwind CSS with Parcel",
  "scripts": {
    "start": " parcel serve src/index.html ",
    "start:mainnet": "NEAR_ENV=mainnet parcel serve src/index.html ",
    "start:testnet": "NEAR_ENV=testnet parcel serve src/index.html ",
    "build": "rm -rf dist && rm -rf .parcel-cache && parcel build --dist-dir ./dist src/index.html ",
    "build:mainnet": "rm -rf dist && rm -rf .parcel-cache && NEAR_ENV=mainnet parcel build --dist-dir ./dist src/index.html ",
    "build:testnet": "rm -rf dist && rm -rf .parcel-cache && NEAR_ENV=testnet parcel build --dist-dir ./dist src/index.html ",
    "clean": "rm -rf .parcel-cache dist"
  },
  "devDependencies": {
    "@parcel/transformer-image": "2.7.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "autoprefixer": "^10.4.12",
    "buffer": "^6.0.3",
    "os-browserify": "^0.3.0",
    "parcel": "2.7.0",
    "postcss": "^8.4.18",
    "process": "0.11.10",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@near-wallet-selector/core": "^7.0.0",
    "@near-wallet-selector/modal-ui": "^7.0.0",
    "@near-wallet-selector/near-wallet": "^7.0.0",
    "@react-icons/all-files": "^4.1.0",
    "@ref_finance/ref-sdk": "^1.0.2",
    "@types/big.js": "^6.1.5",
    "@types/bn.js": "^5.1.1",
    "@types/lodash": "^4.14.182",
    "big.js": "^6.2.1",
    "bn.js": "^5.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mathjs": "^9.3.0",
    "near-api-js": "^0.44.2",
    "postcss-import": "15.0.0",
    "react": "17.0.2",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "17.0.2",
    "rxjs": "^7.5.5",
    "tailwindcss": "^3.2.1"
  },
  "alias": {
    "react": "./node_modules/@ref_finance/ref-sdk/node_modules/react",
    "react-dom": "./node_modules/@ref_finance/ref-sdk/node_modules/react-dom"
  }
}